
<SCENE>


<INITIALS>
    <frustum near="2" far="500"/>
<!--    <translation x="0" y="0" z="0"/>     
    <rotation axis="x" angle="0"/>                 
    <rotation axis="y" angle="0"/>                 
    <rotation axis="z" angle="0"/> -->                  
	<translation x="-100" y="-100" z="-100"/>     
    <rotation axis="x" angle="0"/>                 
    <rotation axis="y" angle="0"/>                 
    <rotation axis="z" angle="0"/>          
    <scale sx="1" sy="1" sz="1"/>                   
    <reference length="5"/>                         
</INITIALS>


<ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1"/>        
    <background r="0" g="0" b="0" a="1"/>           
</ILLUMINATION>


<LIGHTS>
    <LIGHT id="headlight">                          
        <enable value ="1"/>         				
        <position x="4.8" y="8.6" z="4.8" w="1"/>   
        <ambient r="0.4" g="0.4" b="0.4" a="1"/>    
        <diffuse r="0.4" g="0.4" b="0.4" a="1"/>  	
        <specular r="0.5" g="0.5" b="0.5" a="1"/>   
    </LIGHT>
    <LIGHT id="moonlight">                          
        <enable value ="1"/>         				
        <position x="-45" y="40" z="0" w="1"/>      
        <ambient r="0.4" g="0.4" b="0.4" a="1"/>    
        <diffuse r="0.4" g="0.4" b="0.4" a="1"/>  	
        <specular r="0.8" g="0.8" b="0.8" a="1"/>   
    </LIGHT>
</LIGHTS>


<TEXTURES>
    <TEXTURE id="yellow">
        <file path="textures/yellow.jpg"/>    
        <amplif_factor s="1" t="1"/>          
    </TEXTURE>
    <TEXTURE id="blackmetal">
        <file path="textures/blackmetal.jpg"/>
        <amplif_factor s="1" t="1"/>          
    </TEXTURE>
    <TEXTURE id="smiley">
        <file path="textures/smiley.jpg"/>    
        <amplif_factor s="1" t="1"/>          
    </TEXTURE>
    <TEXTURE id="chess">
        <file path="textures/chess.png"/>     
        <amplif_factor s="1" t="1"/>          
    </TEXTURE>
    <TEXTURE id="pattern2">
        <file path="textures/pattern2.png"/>  
        <amplif_factor s="1" t="1"/>          
    </TEXTURE>
    <TEXTURE id="blue">
        <file path="textures/blue.jpg"/>      
        <amplif_factor s="1" t="1"/>          
    </TEXTURE>
    <TEXTURE id="bluefloor">
        <file path="textures/floor.jpg"/>     
        <amplif_factor s="1" t="1"/>          
    </TEXTURE>
    <TEXTURE id="moontext">
        <file path="textures/moon.jpg"/>      
        <amplif_factor s="1" t="1"/>          
    </TEXTURE>
</TEXTURES>


<MATERIALS>
    <MATERIAL id="branco">                                  
        <shininess value="1"/>
        <specular r="1" g="1" b="1" a="1"/>           
        <diffuse r="1" g="1" b="1" a="1"/>            
        <ambient r="1" g="1" b="1" a="1"/>            
        <emission r="0" g="0" b="0" a="1"/>           
    </MATERIAL>
    <MATERIAL id="branco_brilhante">                            
        <shininess value="1"/>
        <specular r="1" g="1" b="1" a="1"/>           
        <diffuse r="1" g="1" b="1" a="1"/>            
        <ambient r="1" g="1" b="1" a="1"/>            
        <emission r="0" g="0" b="0" a="1"/>           
    </MATERIAL>
</MATERIALS>


<LEAVES>
    <LEAF id="square" type="rectangle" args="-0.5 0.5 0.5 -0.5"/>
    <LEAF id="rectangle" type="rectangle" args="-1 0.5 1 -0.5"/>
    <LEAF id="cylinder" type="cylinder" args="1 1 1 16 16"/> 
    <LEAF id="cone" type="cylinder" args="1 0.5 1 16 16"/> 
    <LEAF id="sphere" type="sphere" args="1 16 16"/>
	<!--<LEAF id="triangle" type="triangle" args="0 0 0 1 0 0 0 1 0"/> -->
    <!-- Parameters of the above primitives: -->
        <!-- rectangle: 2D coordinates for left-top and right-bottom vertices -->
        <!-- cylinder: height, bottom radius, top radius, sections along height, parts per section -->
        <!-- sphere: radius, parts along radius, parts per section -->
        <!-- triangle: coordinates of each vertex -->
</LEAVES>


<NODES>

    <ROOT id="scene"/>
                      

    <NODE id="scene">
        <MATERIAL id="null"/>
        <TEXTURE id="null"/>
		<DESCENDANTS>
            <DESCENDANT id="floor"/>
            <DESCENDANT id="wall"/>
            <!--<DESCENDANT id="monster"/>-->
            <DESCENDANT id="moon"/>
        </DESCENDANTS>
    </NODE>
    
    <NODE id="floor">
        <MATERIAL id="branco"/>
        <TEXTURE id="bluefloor"/>
        <TRANSLATION x="3.3" y="0" z="3.3"/>
        <ROTATION axis="y" angle="45"/>
        <ROTATION axis="x" angle="-90"/>
        <SCALE sx="65" sy="80" sz="1"/>
		<DESCENDANTS>
            <DESCENDANT id="rectangle"/>
        </DESCENDANTS>
    </NODE>

    <NODE id="wall">
        <MATERIAL id="branco"/>
        <TEXTURE id="blue"/>
        <TRANSLATION x="-25" y="25" z="-25"/>
        <ROTATION axis="y" angle="45"/>
        <SCALE sx="65" sy="50" sz="1"/>
		<DESCENDANTS>
            <DESCENDANT id="rectangle"/>
        </DESCENDANTS>
    </NODE>

    <NODE id="moon">
        <MATERIAL id="branco_brilhante"/>
        <TEXTURE id="moontext"/>
        <TRANSLATION x="-45" y="40" z="0"/>
        <SCALE sx="3" sy="3" sz="3"/>
		<DESCENDANTS>
            <DESCENDANT id="sphere"/>
        </DESCENDANTS>
    </NODE>

    

</NODES>


</SCENE>
